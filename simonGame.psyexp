      <CodeComponent name="generateSeqMat_Grammar" plugin="None">
        <Param val="import numpy as np&amp;#10;import itertools&amp;#10;import random&amp;#10;&amp;#10;nWord = 7&amp;#10;nTrial = 13&amp;#10;nPrac = 3&amp;#10;nEle = nWord * 3&amp;#10;&amp;#10;dist_letter2center = 0.15&amp;#10;&amp;#10;### set group index: 0 / 1&amp;#10;groupIdx = np.random.randint(low=0, high=2)&amp;#10;#groupIdx = 1&amp;#10;&amp;#10;### specify legal words: in order T, I, N, O&amp;#10;gram_legal = [[0, 1, 2], [0, 3, 2], [2, 1, 0], [2, 3, 0]]&amp;#10;gram_illegal = [[1, 2, 3], [3, 2, 1], [1, 0, 3], [3, 0, 1]]&amp;#10;if groupIdx == 0:&amp;#10;    currGram = gram_legal&amp;#10;else:&amp;#10;    currGram = gram_illegal&amp;#10;&amp;#10;&amp;#10;&amp;#10;### set seqMat&amp;#10;seqMat = []&amp;#10;for iTrial in range(nTrial):&amp;#10;    toneSeqCollector = []&amp;#10;    wordSeq = []&amp;#10;    for iWord in range(nWord):&amp;#10;        choose_from = [0, 1, 2, 3]&amp;#10;        if iWord == 0:&amp;#10;            curr_idx = random.choice(choose_from)&amp;#10;        else:&amp;#10;            choose_from.remove(curr_idx)&amp;#10;            curr_idx = random.choice(choose_from)&amp;#10;        wordSeq.append(curr_idx)&amp;#10;        &amp;#10;    for iWord in range(nWord):&amp;#10;        word_id = wordSeq[iWord]&amp;#10;        toneSeqCollector = toneSeqCollector + currGram[word_id]&amp;#10;    seqMat.append(toneSeqCollector)&amp;#10;    &amp;#10;    &amp;#10;### set practice mat&amp;#10;for iPrac in range(nPrac):&amp;#10;    curr_list = []&amp;#10;    &amp;#10;    for iEle in range(nEle):&amp;#10;        choose_from = [0, 1, 2, 3]&amp;#10;        if iEle == 0:&amp;#10;            curr_idx = random.choice(choose_from)&amp;#10;        else:&amp;#10;            choose_from.remove(curr_idx)&amp;#10;            curr_idx = random.choice(choose_from)&amp;#10;        curr_list.append(curr_idx)&amp;#10;        &amp;#10;    seqMat[iPrac] = curr_list&amp;#10;        &amp;#10;    &amp;#10;    &amp;#10;&amp;#10;&amp;#10;" valType="extendedCode" updates="constant"